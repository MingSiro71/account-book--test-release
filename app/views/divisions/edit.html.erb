<% provide( :title, "#{@division.name}の編集" ) %>
<h1><%= @division.name %>の編集</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with model: @division, method: :patch do |f| %>
      <%= render 'shared/error_messages', data: @division %>
      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.submit "変更反映", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>